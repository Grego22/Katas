<!-- My Solution -->
def well(x)
  good = 0
  x.each do |y|
   good +=1 if y == 'good'
  end
  if good > 2
    return 'I smell a series!'
  elsif good > 0
    return 'Publish!'
  else
    return 'Fail!'
  end
end

