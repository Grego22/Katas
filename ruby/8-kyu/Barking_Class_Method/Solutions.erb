# Change of class, interesting 
class Bark
  def bark()
    "Woof"
  end
end

class Dog < Bark
  def initialize(breed)
    @breed=breed
  end
end

snoopy=Dog.new("Beagle")
scoobydoo=Dog.new("Great Dane")
